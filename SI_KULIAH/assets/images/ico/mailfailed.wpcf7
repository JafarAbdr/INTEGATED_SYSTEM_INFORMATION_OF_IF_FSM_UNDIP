<script type="text/javascript">
	jQuery(document).ready(function($){
    
	    /* ------------------------------------------------------------------------ */
		/* Add PrettyPhoto */
		/* ------------------------------------------------------------------------ */
		
		var lightboxArgs = {			
						animation_speed: 'fast',
						overlay_gallery: true,
			autoplay_slideshow: false,
						slideshow: 5000, /* light_rounded / dark_rounded / light_square / dark_square / facebook */
									theme: 'pp_default', 
									opacity: 0.8,
						show_title: true,
						deeplinking: false,
			allow_resize: true, 			/* Resize the photos bigger than viewport. true/false */
			counter_separator_label: '../../../../default.htm', 	/* The separator for the gallery counter 1 "of" 2 */
			default_width: 940,
			default_height: 529
		};

		/* ------------------------------------------------------------------------ */
		/* Count customers */
		/* ------------------------------------------------------------------------ */
		function trustedClients(toAppend) {

			var supr = '../../../../../marketplace.envato.com/api/v3/item_3A2834580.json';
			var hurr = '../../../../../marketplace.envato.com/api/v3/item_3A2244903.json';

			$.getJSON( supr, function(data) {
				//success
				localStorage.setItem('suprSales', data.item.sales);
			});
			$.getJSON( hurr, function(data) {
				//success
				localStorage.setItem('hurrSales', data.item.sales);
			});
			var totalSales = Number(localStorage.getItem('suprSales')) + Number(localStorage.getItem('hurrSales'));
			$(toAppend).html(totalSales);

		}
		
		trustedClients('.count');
		
					$('a[href$=jpg], a[href$=JPG], a[href$=jpeg], a[href$=JPEG], a[href$=png], a[href$=gif], a[href$=bmp]:has(img)').prettyPhoto(lightboxArgs);
				
		$('a[class^="prettyPhoto"], a[rel^="prettyPhoto"]').prettyPhoto(lightboxArgs);
		
				
	});
